using System;

namespace Assignment
{
    class Alpha 
    {
    public static void Main(string[] args){
float[]alpha = new float [50];
for(int i = 0; i < 25; i++){
    alpha [i] = i * i;
}
  for (int i = 25; i< alpha.Length; i++) {
      alpha [i] = i * 3;
  }
  for (int i = 0; i< alpha.Length; i++) {
      Console.Write(alpha [i] + " ");
      if ((i+1)% 10 == 0){
          Console.WriteLine();
      }
      }
    }
}
}
